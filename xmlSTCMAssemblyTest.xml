<?xml version="1.0" encoding="UTF-8"?>
<stcmassembly>
	<declare>
		<component name= "IHello">
			<dataOut name="sayHello" type="string"/>
		</component>

		<instance name="clientHello" componentRef="IHello" path="/Clients/helloWorld" signature=""/>

		<configPort>
			<inout>
				<setPort in="" out="clientHello.sayHello"/>
			</inout>
		</configPort>
	</declare>
</stcmassembly>
